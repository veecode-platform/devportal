{
  "name": "root",
  "version": "1.0.0",
  "private": true,
  "engines": {
    "node": "22"
  },
  "scripts": {
    "init-local": "make full && yarn check-dynamic-plugins",
    "check-dynamic-plugins": "./scripts/check_dynamic_plugins.sh",
    "dev": "NODE_OPTIONS=--no-node-snapshot turbo run start --filter=backend --filter=app --config ../../app-config.yaml --config ../../dynamic-plugins-root/app-config.dynamic-plugins.yaml",
    "dev-local": "NODE_OPTIONS=--no-node-snapshot turbo run start --filter=backend --filter=app -- --config ../../app-config.local.yaml --config ../../dynamic-plugins-root/app-config.dynamic-plugins.yaml",
    "start": "NODE_OPTIONS=--no-node-snapshot turbo run start --filter=backend",
    "start-backend": "yarn workspace backend start",
    "build": "turbo run build",
    "build:backend": "yarn tsc && yarn workspace backend build",
    "build-image": "yarn workspace backend build-image",
    "tsc": "turbo run tsc",
    "export-dynamic": "echo \"Command moved to dynamic-plugins/package.json\"",
    "export-dynamic:clean": "echo \"Command moved to dynamic-plugins/package.json\"",
    "copy-dynamic-plugins": "echo \"Command moved to dynamic-plugins/package.json\"",
    "clean": "turbo run clean",
    "test": "turbo run test",
    "lint:check": "turbo run lint:check",
    "lint:fix": "turbo run lint:fix",
    "lint": "turbo run lint",
    "tsc:full": "tsc --skipLibCheck false --incremental false",
    "diff": "lerna run diff --",
    "test:all": "lerna run test -- --coverage",
    "lint:all": "backstage-cli repo lint",
    "prettier:check": "prettier --check .",
    "new": "backstage-cli new --scope internal",
    "remove-plugin": "backstage-cli remove-plugin",
    "clear-deps": "find .  -path ./.docker-data -prune -o -name node_modules -type d -exec rm -rf '{}' +",
    "docker-up": "docker compose --profile=complete up -d",
    "docker-down": "docker compose --profile=complete down",
    "check": "yarn upgrade-interactive",
    "update-backstage": "backstage-cli versions:bump --pattern '@{backstage,roadiehq,backstage-community,veecode-platform}/*'"
  },
  "workspaces": {
    "packages": [
      "packages/*",
      "plugins/*"
    ]
  },
