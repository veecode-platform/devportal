{
  "name": "dynamic-plugins-root",
  "version": "1.7.0",
  "private": true,
  "scripts": {
    "build": "yarn tsc && yarn export-dynamic",
    "tsc": "yarn workspaces foreach --all run tsc",
    "export-dynamic": "yarn workspaces foreach --all run export-dynamic",
    "clean": "yarn workspaces foreach --all run clean"
  },
  "workspaces": {
    "packages": [
      "_utils",
      "wrappers/*"
    ]
  },
  "devDependencies": {
    "@backstage/cli": "0.30.0",
    "@janus-idp/cli": "3.6.1",
    "fs-extra": "^11.2.0",
    "typescript": "^5.4.0",
    "yaml": "^2.4.5"
  },
  "dependencies": {
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-router-dom": "^6.25.1"
  },
  "resolutions": {
    "@types/react": "18.3.23",
    "@types/react-dom": "18.3.7"
  }
}
